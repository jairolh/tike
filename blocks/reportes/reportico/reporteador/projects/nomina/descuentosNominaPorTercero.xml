   liq_mes mes,
   (qui_cod||' '||qui_nombre) quincena,   
   des_tipoemple tipo,      
   con_cod codigo,
   trim(con_nombre) concepto,
   emp_nro_iden cedula,
   trim(emp_nombre) nombre,
   liq_valor total
from petip_quin, prcon, prliquid, prhisempcierre, peemp, pecargodes 
where qui_cod = liq_tq_cod
   and con_tipo = liq_con_tipo
   and con_cod = liq_con_cod
   and con_tipo = 2
   [and liq_ano = {ANO}]                    
   [and liq_mes = {MES}]                      
   [and liq_tq_cod = {QUINCENA}]                    
   [and con_nombre = {CONCEPTO}]  
   and liq_emp_cod = hec_emp_cod   
   and liq_ano = hec_liq_ano
   and liq_mes = hec_liq_mes
   and liq_tq_cod = hec_liq_tq_cod
   and hec_emp_cod = emp_cod
   and hec_car_tc_cod = des_tc_cod
order by 1,2,3,4,5,7 asc</SQLRaw></SQL><QueryColumns><QueryColumn><Name>ano</Name><TableName></TableName><ColumnName>liq_ano</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>mes</Name><TableName></TableName><ColumnName>liq_mes</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>quincena</Name><TableName></TableName><ColumnName>(qui_cod||' '||qui_nombre)</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>tipo</Name><TableName></TableName><ColumnName>des_tipoemple</ColumnName><ColumnType>char</Co