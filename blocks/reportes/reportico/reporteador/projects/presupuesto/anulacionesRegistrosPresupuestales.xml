<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>Anulaciones Parciales y Totales</ReportTitle><ReportDescription></ReportDescription><PageSize>.DEFAULT</PageSize><PageOrientation>.DEFAULT</PageOrientation><TopMargin></TopMargin><BottomMargin></BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><pdfFont></pdfFont><pdfFontSize></pdfFontSize><PreExecuteCode>NONE</PreExecuteCode><formBetweenRows>solidline</formBetweenRows><gridDisplay>.DEFAULT</gridDisplay><gridSortable>.DEFAULT</gridSortable><gridSearchable>.DEFAULT</gridSearchable><gridPageable>.DEFAULT</gridPageable><gridPageSize>.DEFAULT</gridPageSize></Format><Datasource><SourceType>database</SourceType><SourceConnection><DatabaseType>mysql</DatabaseType><DatabaseName></DatabaseName><HostName></HostName><ServiceName></ServiceName><UserName></UserName><Password></Password></SourceConnection></Datasource><EntryForm><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection>all</RowSelection><SQL><QuerySql></QuerySql><SQLRaw>SELECT rub.vigencia vigencia,
   ran.codigo_unidad_ejecutora unidad_ejecutora,
   rub.interno_rubro rubro_interno,
   (codigo_nivel1||'-'||codigo_nivel2||'-'||codigo_nivel3||'-'||codigo_nivel4||'-'||codigo_nivel5||'-'||codigo_nivel6||'-'||codigo_nivel7||'-'||codigo_nivel8) rubro,
   rub.descripcion descripcion,
   ran.numero_disponibilidad cdp,
   ran.numero_registro rp,
   ran.valor_anulado,
   ran.fecha_anulacion fecha_anulacion,
   'ANULACION TOTAL' tipo_anulacion,
   ran.motivo motivo
FROM pr.pr_v_rubros rub, pr.pr_rp_anulados ran
WHERE
1=1
 [AND EXTRACT (YEAR FROM fecha_anulacion) IN ({anio})]
AND rub.vigencia = ran.vigencia
   AND rub.interno_rubro = ran.rubro_interno
   [AND ran.codigo_unidad_ejecutora IN ({unidadEjecutora})]
UNION
SELECT rub.vigencia vigencia,
   rdis.codigo_unidad_ejecutora unidad_ejecutora,
   rub.interno_rubro rubro_interno,
   (codigo_nivel1||'-'||codigo_nivel2||'-'||codigo_nivel3||'-'||codigo_nivel4||'-'||codigo_nivel5||'-'||codigo_nivel6||'-'||codigo_nivel7||'-'||codigo_nivel8) rubro,
   rub.descripcion descripcion,
   rdis.numero_disponibilidad cdp,
   rdis.numero_registro rp,
   anu.valor valor_anulado,
   anu.fecha_registro fecha_anulacion,
   'ANULACION PARCIAL' tipo_anulacion,
   anu.descripcion motivo
FROM pr.pr_v_rubros rub, pr.pr_registro_disponibilidad rdis, pr.pr_anulaciones anu
WHERE 
1 = 1
[AND EXTRACT (YEAR FROM  anu.fecha_registro) IN ({anio})]
AND rub.vigencia = rdis.vigencia
   AND rub.interno_rubro = rdis.rubro_interno
   [AND rdis.codigo_unidad_ejecutora IN ({unidadEjecutora})]
   AND rdis.vigencia = anu.vigencia
   AND rdis.codigo_unidad_ejecutora = anu.codigo_unidad_ejecutora
   AND rdis.numero_registro = anu.numero_documento_anulado
   AND anu.documento_anulado = 'REGISTRO'
ORDER BY 1,2,3,8,6 ASC</SQLRaw></SQL><QueryColumns><QueryColumn><Name>vigencia</Name><TableName>rub</TableName><ColumnName>vigencia</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>unidad_ejecutora</Name><TableName>ran</TableName><ColumnName>codigo_unidad_ejecutora</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>rubro_interno</Name><TableName>rub</TableName><ColumnName>interno_rubro</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>rubro</Name><TableName></TableName><ColumnName>(codigo_niv