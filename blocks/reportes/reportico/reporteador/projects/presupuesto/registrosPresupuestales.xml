FROM PR.PR_UNIDAD_EJECUTORA
WHERE 1 = 1</SQLRaw><SQL><QuerySql></QuerySql></SQL><QueryColumns><QueryColumn><Name>codigo</Name><TableName></TableName><ColumnName>codigo</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><justify>left</justify></Format></QueryColumn></QueryColumns><OrderColumns></OrderColumns><Assignments></Assignments></Query><CriteriaLinks></CriteriaLinks></CriteriaItem><CriteriaItem><Name>rubro</Name><Title>Rubro</Title><QueryTableName></QueryTableName><QueryColumnName>V.INTERNO_RUBRO</QueryColumnName><CriteriaType>LOOKUP</CriteriaType><CriteriaDisplay>DROPDOWN</CriteriaDisplay><ExpandDisplay>MULTI</ExpandDisplay><ReturnColumn>COD_RUBRO</ReturnColumn><DisplayColumn>RUBRO</DisplayColumn><OverviewColumn>NOMBRE</OverviewColumn><MatchColumn>NOMBRE</MatchColumn><CriteriaDefaults></CriteriaDefaults><CriteriaList></CriteriaList><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection></RowSelection><SQLRaw>SELECT RUBROS.COD_RUBRO,
RUBROS.NOMBRE,
RUBROS.RUBRO
FROM (       

SELECT DISTINCT RUB.INTERNO AS COD_RUBRO,
RUB.DESCRIPCION AS NOMBRE ,
       N1.CODIGO||'.'||
       N2.CODIGO||'.'||
       N3.CODIGO||'.'||
       N4.CODIGO||'.'||
       N5.CODIGO||'.'||
       N6.CODIGO||'.'||
       N7.CODIGO||'.'||
       N8.CODIGO||' - '||
       TRIM(RUB.DESCRIPCION) AS RUBRO
FROM	PR_RUBRO RUB
INNER JOIN PR_NIVEL1 N1 ON RUB.INTERNO_NIVEL1 = N1.INTERNO AND  RUB.VIGENCIA = N1.VIGENCIA AND N1.CODIGO=3 
INNER JOIN PR_NIVEL2 N2 ON RUB.INTERNO_NIVEL2 = N2.INTERNO AND  RUB.VIGENCIA = N2.VIGENCIA
INNER JOIN PR_NIVEL3 N3 ON RUB.INTERNO_NIVEL3 = N3.INTERNO AND  RUB.VIGENCIA = N3.VIGENCIA
INNER JOIN PR_NIVEL4 N4 ON RUB.INTERNO_NIVEL4 = N4.INTERNO AND  RUB.VIGENCIA = N4.VIGENCIA
INNER JOIN PR_NIVEL5 N5 ON RUB.INTERNO_NIVEL5 = N5.INTERNO AND  RUB.VIGENCIA = N5.VIGENCIA
INNER JOIN PR_NIVEL6 N6 ON RUB.INTERNO_NIVEL6 = N6.INTERNO AND  RUB.VIGENCIA = N6.VIGENCIA
INNER JOIN PR_NIVEL7 N7 ON RUB.INTERNO_NIVEL7 = N7.INTERNO AND  RUB.VIGENCIA = N7.VIGENCIA
INNER JOIN PR_NIVEL8 N8 ON RUB.INTERNO_NIVEL8 = N8.INTERNO AND  RUB.VIGENCIA = N8.VIGENCIA
) RUBROS
ORDER BY 3
</SQLRaw><SQL><QuerySql></QuerySql></SQL><QueryColumns><QueryColumn><Name>COD_RUBRO</Name><TableName>RUBROS</TableName><ColumnName>COD_RUBRO</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><justify>left</justify></Format></QueryColumn><QueryColumn><Name>RUBRO</Name><TableName>RUBROS</TableName><ColumnName>RUBRO</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><justify>left</justify></Format></QueryColumn><QueryColumn><Name>NOMBRE</Name><TableName>RUBROS</TableName><ColumnName>NOMBRE</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><justify>left</justify></Format></QueryColumn></QueryColumns><OrderColumns></OrderColumns><Assignments></Assignments></Query><CriteriaLinks></CriteriaLinks></CriteriaItem><CriteriaItem><Name>mes</Name><Title>Mes Inicio</Title><QueryTableName></QueryTableName><QueryColumnName></QueryColumnName><CriteriaType>LIST</CriteriaType><CriteriaDisplay>DROPDOWN</CriteriaDisplay><ExpandDisplay>NOINPUT</ExpandDisplay><ReturnColumn></ReturnColumn><DisplayColumn></DisplayColumn><OverviewColumn></OverviewColumn><MatchColumn></MatchColumn><CriteriaDefaults></CriteriaDefaults><CriteriaList>=,Enero=01,Febrero=02,Marzo=03,Abril=04,Mayo=05,Junio=06,Julio=07,Agosto=08,Septiembre=09,Octubre=10,Noviembre=11,Diciembre=12</CriteriaList><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection></RowSelection><SQLRaw></SQLRaw><SQL><QuerySql></QuerySql></SQL><QueryColumns></QueryColumns><OrderColumns></OrderColumns><Assignments></Assignments></Query><CriteriaLinks></CriteriaLinks></CriteriaItem><CriteriaItem><Name>mesFinal</Name><Title>Mes Final</Title><QueryTableName></QueryTableName><QueryColumnName></QueryColumnName><CriteriaType>LIST</Cr