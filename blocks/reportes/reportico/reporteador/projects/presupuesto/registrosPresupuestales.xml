<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>Reporte Registros Presupuestales</ReportTitle><ReportDescription>Listado de Registros presupuestales por Rubro</ReportDescription><PageSize>US-Legal</PageSize><PageOrientation>Landscape</PageOrientation><TopMargin></TopMargin><BottomMargin></BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><pdfFont></pdfFont><pdfFontSize>6</pdfFontSize><PreExecuteCode>NONE</PreExecuteCode><formBetweenRows>solidline</formBetweenRows><gridDisplay>show</gridDisplay><gridSortable>yes</gridSortable><gridSearchable>yes</gridSearchable><gridPageable>.DEFAULT</gridPageable><gridPageSize>.DEFAULT</gridPageSize></Format><Datasource><SourceType>database</SourceType><SourceConnection><DatabaseType>mysql</DatabaseType><DatabaseName></DatabaseName><HostName></HostName><ServiceName></ServiceName><UserName></UserName><Password></Password></SourceConnection></Datasource><EntryForm><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection>all</RowSelection><SQL><QuerySql></QuerySql><SQLRaw>SELECT D.VIGENCIA,
D.CODIGO_COMPANIA,
D.CODIGO_UNIDAD_EJECUTORA,
D.NUMERO_DISPONIBILIDAD NUMERO_DISPONIBILIDAD,
TO_NUMBER(RD.NUMERO_REGISTRO) NUMERO_REGISTRO,
TO_CHAR(R.FECHA_REGISTRO,'DD/MM/YYYY') FECHA_REGISTRO,
substr(R.numero_compromiso,1,10) numero_compromiso,
C.NUMERO_DOCUMENTO  NUMERO_DOCUMENTO_BENEFICIARIO ,
C.TIPO_DOCUMENTO TIPO_DOCUMENTO_BENEFICIARIO,
REPLACE(substr(D.OBJETO,1,100),CHR(10),'') OBJETO,
TO_CHAR(C.FECHA_INICIAL,'DD/MM/YYYY') FECHA_INICIAL,
TO_CHAR(C.FECHA_FINAL,'DD/MM/YYYY') FECHA_FINAL,
P.NUMERO_DOCUMENTO NUMERO_DOCUMENTO_RESPONSABLE,
P.TIPO_DOCUMENTO TIPO_DOCUMENTO_RESPONSABLE,
v.interno_rubro INTERNO_RUBRO,
V.CODIGO_NIVEL1||'-'||V.CODIGO_NIVEL2||'-'||V.CODIGO_NIVEL3||'-'||V.CODIGO_NIVEL4||'-'||V.CODIGO_NIVEL5||'-'||V.CODIGO_NIVEL6||'-'||V.CODIGO_NIVEL7||'-'||V.CODIGO_NIVEL8 CODIGO_RUBRO,
v.descripcion DESCRIPCION_RUBRO,
SUM(NVL(RP_ANULADO.VALOR,0)) VALOR_ANULADO,
sum(RD.valor) VALOR_TOTAL, 
'ENTE_AUTONOMO'  CLASIFICACION,
sum(RD.valor) VALOR,
C.MOD_SEL MODALIDAD_SELECCION
FROM PR.PR_DISPONIBILIDADES D INNER JOIN PR.PR_DISPONIBILIDAD_RUBRO DR ON 
D.VIGENCIA =DR.VIGENCIA
AND D.CODIGO_COMPANIA=DR.CODIGO_COMPANIA
AND D.CODIGO_UNIDAD_EJECUTORA=DR.CODIGO_UNIDAD_EJECUTORA
AND D.NUMERO_DISPONIBILIDAD=DR.NUMERO_DISPONIBILIDAD
AND D.CODIGO_COMPANIA=230
INNER JOIN PR_V_RUBROS V ON
V.INTERNO_RUBRO=DR.RUBRO_INTERNO
AND V.VIGENCIA=DR.VIGENCIA
INNER JOIN PR.PR_REGISTRO_DISPONIBILIDAD RD ON 
DR.VIGENCIA=RD.VIGENCIA
AND DR.RUBRO_INTERNO=RD.RUBRO_INTERNO
AND DR.CODIGO_COMPANIA=RD.CODIGO_COMPANIA
AND DR.CODIGO_UNIDAD_EJECUTORA=RD.CODIGO_UNIDAD_EJECUTORA
AND DR.NUMERO_DISPONIBILIDAD=RD.NUMERO_DISPONIBILIDAD
INNER JOIN PR.PR_REGISTRO_PRESUPUESTAL R ON 
RD.VIGENCIA=R.VIGENCIA
AND RD.CODIGO_COMPANIA=R.CODIGO_COMPANIA
AND RD.CODIGO_UNIDAD_EJECUTORA=R.CODIGO_UNIDAD_EJECUTORA
AND RD.NUMERO_REGISTRO=R.NUMERO_REGISTRO
AND RD.NUMERO_DISPONIBILIDAD=R.NUMERO_DISPONIBILIDAD
INNER JOIN PR.PR_COMPROMISOS C ON
R.VIGENCIA=C.VIGENCIA
AND R.CODIGO_COMPANIA=C.CODIGO_COMPANIA
AND R.CODIGO_UNIDAD_EJECUTORA=C.CODIGO_UNIDAD_EJECUTORA
AND R.NUMERO_REGISTRO= C.NUMERO_REGISTRO
AND R.NUMERO_COMPROMISO=C.NUMERO_COMPROMISO
AND R.TIPO_COMPROMISO=C.TIPO_COMPROMISO
INNER JOIN shd_informacion_basica S ON 
C.NUMERO_DOCUMENTO=S.IB_CODIGO_IDENTIFICACION
AND C.TIPO_DOCUMENTO=S.IB_TIPO_IDENTIFICACION
INNER JOIN PR.PR_PERSONAS_CDP  P ON 
D.ID_RESPONSABLE_PRESUPUESTO=P.SECUENCIAL
LEFT OUTER JOIN (
SELECT  
  CDP_ANULA.VIGENCIA,
  CDP_ANULA.CODIGO_COMPANIA,
  CDP_ANULA.CODIGO_UNIDAD_EJECUTORA UNIDAD,
  CDP_ANULA.RUBRO_INTERNO RUBRO,
  CDP_ANULA.NUMERO_DISPONIBILIDAD CDP,
  SUM(NVL(CDP_ANULA.VALOR_ANULADO,0)) VALOR
  FROM PR.PR_RP_ANULADOS CDP_ANULA
  GROUP BY CDP_ANULA.VIGENCIA,
  CDP_ANULA.CODIGO_COMPANIA,
  CDP_ANULA.RUBRO_INTERNO,
  CDP_ANULA.CODIGO_UNIDAD_EJECUTORA,
  CDP_ANULA.NUMERO_DISPONIBILIDAD
) RP_ANULADO
ON RP_ANULADO.VIGENCIA=D.VIGENCIA
  AND RP_ANULADO.CODIGO_COMPANIA=D.CODIGO_COMPANIA
  AND RP_ANULADO.UNIDAD=D.CODIGO_UNIDAD_EJECUTORA
  AND RP_ANULADO.RUBRO=V.INTERNO_RUBRO
  AND RP_ANULADO.CDP=D.NUMERO_DISPONIBILIDAD
WHERE D.CODIGO_COMPANIA = 230
[AND TO_CHAR(FECHA_INICIAL,'MM') BETWEEN {mes} AND {mesFinal}]
[AND TO_CHAR(FECHA_FINAL,'MM') BETWEEN {mes} AND {mesFinal}]
GROUP BY RD.NUMERO_REGISTRO ,D.VIGENCIA,
D.CODIGO_COMPANIA,
D.CODIGO_UNIDAD_EJECUTORA,
D.NUMERO_DISPONIBILIDAD,
D.REGISTRO_SIPD,
TO_CHAR(R.FECHA_REGISTRO,'DD/MM/YYYY'),
r.numero_compromiso,
C.NUMERO_DOCUMENTO ,
C.TIPO_DOCUMENTO,
D.OBJETO,
TO_CHAR(C.FECHA_INICIAL,'DD/MM/YYYY') ,
TO_CHAR(C.FECHA_FINAL,'DD/MM/YYYY'),
P.NUMERO_DOCUMENTO,
P.TIPO_DOCUMENTO,
V.INTERNO_RUBRO,
V.CODIGO_NIVEL1||'-'||V.CODIGO_NIVEL2||'-'||V.CODIGO_NIVEL3||'-'||V.CODIGO_NIVEL4||'-'||V.CODIGO_NIVEL5||'-'||V.CODIGO_NIVEL6||'-'||V.CODIGO_NIVEL7||'-'||V.CODIGO_NIVEL8,
v.descripcion,
C.MOD_SEL
ORDER BY TO_NUMBER(RD.NUMERO_REGISTRO)</SQLRaw></SQL><QueryColumns><QueryColumn><Name>VIGENCIA</Name><TableName>D</TableName><ColumnName>VIGENCIA</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>CODIGO_COMPANIA</Name><TableName>D</TableName><ColumnName>CODIGO_COMPANIA</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>CODIGO_UNIDAD_EJECUTORA</Name><TableName>D</TableName><ColumnName>CODIGO_UNIDAD_EJECUTORA</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>NUMERO_DISPONIBILIDAD</Name><TableNam